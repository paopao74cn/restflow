<ivysettings>
	<settings defaultResolver="all"/>
	<resolvers>
		<chain name="all" returnFirst="true">
			<chain name="ssrl_repository" returnFirst="true">
				<url name="Ssrl published repository">
					<ivy pattern="http://smb.slac.stanford.edu/ivy/release/[organisation]/[module]/[revision]/ivy.xml"/>
					<artifact pattern="http://smb.slac.stanford.edu/ivy/release/[organisation]/[module]/[revision]/[artifact].[ext]"/>
				</url>
			</chain>
			<chain name="chained" returnFirst="true">
				<url name="com.springsource.repository.bundles.release">
					<ivy pattern="http://repository.springsource.com/ivy/bundles/release/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]"/>
					<artifact pattern="http://repository.springsource.com/ivy/bundles/release/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]"/>
				</url>
				<url name="com.springsource.repository.bundles.external">
					<ivy pattern="http://repository.springsource.com/ivy/bundles/external/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]"/>
					<artifact pattern="http://repository.springsource.com/ivy/bundles/external/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]"/>
				</url>
				<url m2compatible="true" name="Maven repository no classifier">
					<artifact pattern="http://repo1.maven.org/maven2/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]"/>
				</url>
				<url m2compatible="true" name="Maven repository with classifier">
					<artifact pattern="http://repo1.maven.org/maven2/[organisation]/[module]/[revision]/[artifact]-[revision]-[classifier].[ext]"/>
				</url>
				<ibiblio m2compatible="true" name="codehaus" root="http://repository.codehaus.org/"/>
				<ibiblio m2compatible="true" name="snapshots.codehaus" root="http://snapshots.repository.codehaus.org/"/>
				<ibiblio m2compatible="true" name="apache" root="http://people.apache.org/repo/m2-ibiblio-rsync-repository/"/>
				<ibiblio m2compatible="true" name="apache-incubating" root="http://people.apache.org/repo/m2-incubating-repository/"/>
				<ibiblio name="ibiblio"/>
			</chain>
		</chain>
	</resolvers>
	<modules>
		<module resolver="ssrl_repository" organisation="ssrl" name="*"/>
		<module resolver="ssrl_repository" organisation="org.restflow" name="*"/>
	</modules>
</ivysettings>
